<?xml version="1.0" encoding="UTF-8"?>
<sitemapindex xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    {% for page in pages %}
    <sitemap>
        <loc>{{page[0]|safe}}</loc>
        <lastmod>{{page[1]|datetimeformat('YYYY-MM-DD')}}</lastmod>
    </sitemap>
    {% endfor %}
</sitemapindex>
